# جمعية - متغيرات البيئة المحلية

# وضع التطبيق
VITE_APP_ENV=development

# معلومات التطبيق
VITE_APP_NAME=جمعية
VITE_APP_VERSION=1.0.0

# الإعدادات المحلية
VITE_DEFAULT_LANGUAGE=ar
VITE_DEFAULT_CURRENCY=DZD

# الميزات
VITE_ENABLE_KYC=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_NOTIFICATIONS=true

# التوقيت
VITE_TIMEZONE=Africa/Algiers
VITE_DATE_FORMAT=DD/MM/YYYY
