# جمعية - متغيرات البيئة

# وضع التطبيق (development, production, test)
VITE_APP_ENV=development

# عنوان API (للاستخدام المستقبلي)
# VITE_API_URL=https://api.jamiya.dz

# مفاتيح API خارجية (للاستخدام المستقبلي)
# VITE_PAYMENT_GATEWAY_KEY=
# VITE_SMS_API_KEY=
# VITE_PUSH_NOTIFICATION_KEY=

# إعدادات التطبيق
VITE_APP_NAME=جمعية
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LANGUAGE=ar
VITE_DEFAULT_CURRENCY=DZD

# إعدادات الميزات
VITE_ENABLE_KYC=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_NOTIFICATIONS=true

# إعدادات التوقيت
VITE_TIMEZONE=Africa/Algiers
VITE_DATE_FORMAT=DD/MM/YYYY
